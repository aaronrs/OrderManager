<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.astechdesign.cms">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CMSActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--<activity android:name=".CustomerActivity"></activity>-->
        <!--<activity android:name=".OrderActivity"></activity>-->
        <!--<activity android:name=".NewOrderActivity"></activity>-->
        <provider
            android:authorities="net.astechdesign.cms.database.provider"
            android:name="net.astechdesign.cms.database.providers.CMSDataProvider"/>
    </application>

</manifest>
